@model IEnumerable<IGrouping<string, GameHub.Models.Game>>
@{
    ViewBag.Title = "Browse";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Lista Gier</h2>

@foreach (var group in Model)
{
    <h3>@group.Key</h3>
    <ul>
        @foreach (var game in group)
        {
            <li><a href="@Url.Action(game.name, "Game")">@game.name</a></li>
        }
    </ul>
}